{
  "name": "warp-background-agents",
  "version": "1.0.0",
  "description": "A suite of background agents for Warp terminal development and automation",
  "main": "index.js",
  "scripts": {
    "dev": "node src/dev-server.js",
    "test": "jest",
    "start": "node src/index.js",
    "build": "webpack --mode production",
    "lint": "eslint src/",
    "format": "prettier --write src/",
    "setup": "node scripts/setup.js",
    "agent:code-review": "node src/agents/code-review.js",
    "agent:test-runner": "node src/agents/test-runner.js",
    "agent:deployment": "node src/agents/deployment.js",
    "agent:monitoring": "node src/agents/monitoring.js"
  },
  "keywords": [
    "warp",
    "terminal",
    "background-agents",
    "automation",
    "development"
  ],
  "author": "Background Agents Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "ws": "^8.14.2",
    "node-cron": "^3.0.3",
    "axios": "^1.6.0",
    "dotenv": "^16.3.1",
    "winston": "^3.11.0",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.6",
    "fs-extra": "^11.1.1",
    "glob": "^10.3.10",
    "yaml": "^2.3.4",
    "json5": "^2.2.3"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "@types/node": "^20.9.0",
    "nodemon": "^3.0.1"
  },
  "engines": {
    "node": ">=18.0.0"
  }
} 